  <script type="text/javascript"><!--
    var EventsStaffPickBoxScrollerItems = [];
    var EventsStaffPickBoxFeedPosition = 0;
    var EventsStaffPickBoxTicker;

    function EventsStaffPickBoxScrollTicker () {

      var currentIndex = EventsStaffPickBoxFeedPosition++;
      var maxElements = 1;

      if (currentIndex >= EventsStaffPickBoxScrollerItems.size()) {
        currentIndex = 0;
        EventsStaffPickBoxFeedPosition = 1;
      }
    
      if (EventsStaffPickBoxScrollerItems[currentIndex]) {
        newItem = new Element('div', {'class': "EventListing clearfix", 'id': "event" + EventsStaffPickBoxScrollerItems[currentIndex].objid, 'style': "margin:0; height: 200px; position: relative; padding: 0px;border:0 none;"}).hide().update(EventsStaffPickBoxScrollerItems[currentIndex].body);

        // Add new item.
        new Insertion.Top($('EventsStaffPickBoxScrollerContainer'), newItem);

        // Poof!  New item!
        new Effect.SlideDown($$('#EventsStaffPickBoxScrollerContainer>div').first());

        // Remove old item(s).
        var numElements = $$("#EventsStaffPickBoxScrollerContainer>div").size();
        if (numElements > maxElements) {
          for (var i = maxElements; i < numElements; i++) {
            var deleteMe = $$("#EventsStaffPickBoxScrollerContainer>div")[i];
            // Remove.
            new Effect.Fade(deleteMe, {afterFinish: function() {deleteMe.remove(); return false;}});
          }
        }
      }
      return false;
    }

    function EventsStaffPickBoxReverseScrollTicker () {

      EventsStaffPickBoxFeedPosition--;
      var currentIndex = EventsStaffPickBoxFeedPosition-1;
      var maxElements = 1;

      if (currentIndex < 0) {
        currentIndex = EventsStaffPickBoxScrollerItems.size();
        EventsStaffPickBoxFeedPosition = EventsStaffPickBoxScrollerItems.size()+1;
      }

      if (EventsStaffPickBoxScrollerItems[currentIndex]) {
        newItem = new Element('div', {'class': "EventListing clearfix", 'id': "event" + EventsStaffPickBoxScrollerItems[currentIndex].objid, 'style': "margin:0; height: 200px; position: relative; padding: 0px;border:0 none;"}).update(EventsStaffPickBoxScrollerItems[currentIndex].body);

        // Hide first item
        new Effect.SlideUp($$('#EventsStaffPickBoxScrollerContainer>div').first());

        // Add new item.
        new Insertion.Bottom($('EventsStaffPickBoxScrollerContainer'), newItem);

        // Remove old item(s).
        var numElements = $$("#EventsStaffPickBoxScrollerContainer>div").size();
        if (numElements > maxElements) {
          for (var i = 0; i < (numElements - maxElements); i++) {
            var deleteMe = $$("#EventsStaffPickBoxScrollerContainer>div")[i];
            // Remove.
            new Effect.Fade(deleteMe, {afterFinish: function() {deleteMe.remove(); return false;}});
          }
        }
      }
      return false;
    }

    Event.observe(document, "dom:loaded", function () {

      // Create item feed.
      $$("#EventsStaffPickBox>div.scrollerItem").each(function (f) {
        var objid = f.readAttribute("data-objid");
        if (objid) {
          EventsStaffPickBoxScrollerItems.push({
            'objid': objid,
            'body':  f.readAttribute("data-body")
          });
        }
      });

      // Prime the ticker.
      EventsStaffPickBoxScrollTicker();

      // Set up periodic updater. (via PeriodicalExecuter)
      if (EventsStaffPickBoxScrollerItems.size() > 1) {

        // Set up controls
        jQuery("#EventsStaffPickBoxPrevControl").click(function() {
          EventsStaffPickBoxTicker.stop(); // kill the timer
          EventsStaffPickBoxReverseScrollTicker();
          // set a new timer
          EventsStaffPickBoxTicker = new PeriodicalExecuter( EventsStaffPickBoxScrollTicker, 10);
          return false;
        });

        jQuery("#EventsStaffPickBoxNextControl").click(function() {
          EventsStaffPickBoxTicker.stop(); // kill the timer
          EventsStaffPickBoxScrollTicker();
          // set a new timer
          EventsStaffPickBoxTicker = new PeriodicalExecuter( EventsStaffPickBoxScrollTicker, 10);
          return false;
        });

        EventsStaffPickBoxTicker = new PeriodicalExecuter( EventsStaffPickBoxScrollTicker, 10);
      }
    });
  //--></script>


  

  
